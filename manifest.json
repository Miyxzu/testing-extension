{
    "manifest_version": 3,
    "name": "Test Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": ["acrylic.css", "dashboard/page.html"],
            "matches": ["<all_urls>"]
        }
    ]
}